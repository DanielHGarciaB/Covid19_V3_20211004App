@page
@model COVID.App.Frontend.Pages.EditProfesorModel
@{
}
<h1>Edicion Profesor</h1>




@foreach(var profesor in Model.Profesores)
{
    <table class="table">
        <tr>
            <td><b>Nombre:</b></td>
            <td>@profesor.nombre</td>
        </tr>
        <tr>
            <td><b>Apellidos:</b></td>
            <td>@profesor.apellido</td>
        </tr>
        <tr>
            <td><b>Edad:</b></td>
            <td>@profesor.edad</td>
        </tr>
        <tr>
             <td><b>Estado COVID:</b></td>
             <td>@profesor.estado</td>
        </tr>  
    </table>
    <a href="/Listas/Profesores">Gestor de Profesores</a>
}